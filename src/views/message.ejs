<div id="messages">
    <% Object.keys(messages).forEach(function (type) { %>
        <ul class="<%= type %>">
            <% messages[type].forEach(function (message) { %>
                <div class="alert alert-<%= message.type %>">
                    <%= message.msg %>
                </div>
                <% }) %>
        </ul>
        <% }) %>
</div>